msgid "continuar_paso_5"
msgstr "Instalar &raquo;"

msgid "continuar_paso_6"
msgstr "Finalizar &raquo;"

msgid "voltar_paso_1"
msgstr "&laquo; Regresar al paso 1"

msgid "voltar_paso_2"
msgstr "&laquo; Regresar al paso 2"

msgid "voltar_paso_3"
